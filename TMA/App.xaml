<Application x:Class="TMA.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TMA"
             xmlns:viewModel="clr-namespace:TMA.MVVM.ViewModel"
             xmlns:view="clr-namespace:TMA.MVVM.View"
             StartupUri="MVVM/View/LoginView.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/ResourceDictionary/ButtonTheme.xaml"/>
